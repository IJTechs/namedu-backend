    "express": "^4.21.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "mongoose": "^8.9.5",
    "morgan": "^1.10.0",
    "redis": "^4.7.0",
    "redis-commander": "^0.8.0",
    "winston": "^3.17.0"
